    "objects": [
        {
            "type": "malware",
            "spec_version": "2.1",
            "id": "malware--5b489130-e72f-4906-be5c-bbbf3259871c",
            "created": "2025-08-11T00:00:00Z",
            "modified": "2025-08-11T00:00:00Z",
            "name": "LameHug",
            "is_family": "false",
            "labels": [
                "infostealer",
                "python-based",
                "ai-assisted",
                "espionage"
            ],
            "description": "LameHug is a Python-based infostealer linked to the Russian state-sponsored group APT28. Discovered in mid-2025, it represents the first publicly documented case of malware operationally integrating a large language model (LLM) to dynamically generate malicious commands at runtime. It is deployed via spear-phishing targeting Ukrainian government agencies and leverages cloud-hosted AI via Hugging Face API for adaptive, signature-less operations."
        },
        {
            "type": "threat-actor",
            "spec_version": "2.1",
            "id": "threat-actor--ca5e6645-cd96-4cc7-99c8-e4da12c4af7d",
            "created": "2025-08-11T00:00:00Z",
            "modified": "2025-08-11T00:00:00Z",
            "name": "APT28",
            "labels": [
                "nation-state",
                "state-sponsored",
                "apt",
                "espionage"
            ],
            "description": "APT28, also known as Fancy Bear or Sofacy, is a Russian state-sponsored advanced persistent threat group. It is known for custom malware and spear-phishing campaigns, and in 2025 deployed the LameHug infostealer in a campaign against Ukrainian government agencies, integrating AI-driven command generation to enhance operational agility."
        },
        {
            "type": "campaign",
            "spec_version": "2.1",
            "id": "campaign--2dcec060-867e-489f-9d3b-c46d828eb10d",
            "created": "2025-08-11T00:00:00Z",
            "modified": "2025-08-11T00:00:00Z",
            "name": "LameHug Spear-Phishing Campaign Against Ukraine",
            "description": "A spear-phishing campaign in 2025 by APT28 targeting Ukrainian government and defense sector entities using the LameHug infostealer with integrated LLM for dynamic command generation. The attack leveraged compromised Ukrainian ministry email accounts and malicious .pif executables disguised as PDFs.",
            "first_seen": "2025-06-01T00:00:00Z",
            "objective": "Espionage and sensitive file exfiltration from Ukrainian government agencies"
        },
        {
            "type": "attack-pattern",
            "spec_version": "2.1",
            "id": "attack-pattern--4bb793dc-74bd-4ec0-8291-a6061b040c7e",
            "created": "2025-08-11T00:00:00Z",
            "modified": "2025-08-11T00:00:00Z",
            "name": "Double-Extension Masquerade",
            "description": "Using files with double extensions to disguise malicious executables as benign documents, such as .pdf.zip containing a .pif executable; aligns with MITRE ATT&CK T1036.",
            "external_references": [
                {
                    "source_name": "mitre-attack",
                    "external_id": "T1036"
                }
            ]
        },
        {
