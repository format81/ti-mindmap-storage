    "objects": [
        {
            "type": "campaign",
            "id": "campaign--f4e4bcc3-d8cf-495a-9da9-4e8e6dea05fc",
            "name": "Malvertising Campaign with Info Stealers",
            "description": "A large-scale malvertising campaign detected by Microsoft Threat Intelligence in December 2024. It leveraged illegal streaming websites embedded with malvertising redirectors to lead users to GitHub, Discord, and Dropbox for information theft. The campaign used modular and multi-stage approaches to deploy malware, collect system information, and exfiltrate data.",
            "first_seen": "2024-12-01T00:00:00Z",
            "last_seen": "2025-01-31T00:00:00Z",
            "objective": "Stealing information from nearly one million devices globally across various industries."
        },
        {
            "type": "malware",
            "id": "malware--5ecec4d9-3ca8-46f6-bfd3-aa15232468c7",
            "name": "Lumma Stealer",
            "is_family": "false",
            "labels": [
                "info-stealer"
            ],
            "description": "Malware used in the campaign for collecting system information and credentials, including memory size, graphic details, screen resolution, OS, and user paths."
        },
        {
            "type": "malware",
            "id": "malware--53cb59de-a6a0-4939-bc41-b27f099a1084",
            "name": "Doenerium",
            "is_family": "true",
            "labels": [
                "info-stealer"
            ],
            "description": "An updated version of Doenerium used alongside Lumma stealer for collecting browser and system information."
        },
        {
            "type": "malware",
            "id": "malware--f3b52f4b-16fd-4a21-870e-d8e4ab675740",
            "name": "NetSupport RAT",
            "is_family": "false",
            "labels": [
                "remote-access-tool"
            ],
            "description": "Remote monitoring and management software deployed with information stealers to facilitate remote access and further data exfiltration."
        },
        {
            "type": "infrastructure",
            "id": "infrastructure--906f5a45-6a13-4358-8bed-41f9381bdf1f",
            "name": "GitHub Repository Hosting",
            "infrastructure_type": "hosting",
            "description": "GitHub repositories used to host initial access payloads for malware delivery in the malvertising campaign."
        },
        {
            "type": "infrastructure",
            "id": "infrastructure--4732588d-b4e9-40d6-bd03-ea75591d4fe5",
            "name": "Discord and Dropbox Hosting",
            "infrastructure_type": "hosting",
            "description": "Discord and Dropbox platforms used as alternative malware delivery points alongside GitHub in the campaign."
        },
        {
            "type": "threat-actor",
            "id": "threat-actor--532e04d9-2d0f-4219-958f-ccb8f6244d37",
            "name": "Storm-0408",
            "labels": [
                "financially-motivated"
            ],
            "description": "Threat actor tracked by Microsoft, involved in spreading remote access and information-stealing malware using phishing, SEO, or malvertising campaigns."
        },
